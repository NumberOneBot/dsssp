{
  "name": "dsssp",
  "type": "module",
  "private": false,
  "version": "0.5.0",
  "author": "Oleksiy Strelets",
  "license": "AGPL-3.0-only",
  "homepage": "https://dsssp.io",
  "description": "React Library for Audio Processing and Visualization",
  "funding": "https://github.com/numberonebot/dsssp?sponsor=1",
  "bugs": {
    "url": "https://github.com/numberonebot/dsssp/issues"
  },
  "keywords": [
    "react",
    "react-components",
    "ui-library",
    "svg",
    "chart",
    "graph",
    "visualization",
    "dsp",
    "signal-processing",
    "digital-signal-processing",
    "web-audio",
    "audio",
    "sound",
    "filters",
    "effects",
    "spectrum",
    "waveform",
    "processing",
    "engineering"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./font": {
      "import": {
        "types": "./dist/font.d.ts",
        "default": "./dist/index.css"
      },
      "require": {
        "types": "./dist/font.d.ts",
        "default": "./dist/index.css"
      }
    }
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "vite build",
    "lint": "tsc && eslint .",
    "lint:fix": "eslint . --fix",
    "lint:complexity": "eslintcc -sr -a -mr=F -mar=F -gt=B \"./src/**/*.{ts,tsx}\"",
    "format": "prettier . --write --ignore-unknown",
    "prepare": "husky",
    "deps": "npx taze",
    "deps:update": "npx taze -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "version-minor": "npm version minor",
    "version-patch": "npm version patch",
    "version-prepatch": "npm version prepatch --preid=alpha",
    "publish": "npm publish",
    "publish:alpha": "npm publish --tag alpha"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@fontsource-variable/dm-sans": "^5.1.1",
    "@fontsource-variable/reddit-mono": "^5.1.1",
    "@fontsource/iosevka-etoile": "^5.1.0",
    "@fontsource/monaspace-krypton": "^5.1.0",
    "@storybook/addon-actions": "^8.5.8",
    "@storybook/addon-docs": "^8.5.8",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/builder-vite": "^8.5.8",
    "@storybook/manager-api": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/theming": "^8.5.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.3",
    "eslintcc": "^0.8.3",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "storybook": "^8.5.8",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
