{
  "name": "dsssp",
  "private": false,
  "version": "0.1.4",
  "description": "React Library for Audio Processing and Visualization",
  "keywords": [
    "react",
    "react-components",
    "ui-library",
    "svg",
    "chart",
    "graph",
    "visualization",
    "dsp",
    "signal-processing",
    "digital-signal-processing",
    "sound",
    "waveform",
    "web-audio",
    "audio-filters",
    "audio-effects",
    "audio-spectrum",
    "audio-processing",
    "audio-engineering",
    "music-technology"
  ],
  "author": "Oleksiy Strelets",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./font": {
      "import": {
        "types": "./dist/font.d.ts",
        "default": "./dist/style.css"
      },
      "require": {
        "types": "./dist/font.d.ts",
        "default": "./dist/style.css"
      }
    }
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier . --write --ignore-unknown",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@fontsource-variable/dm-sans": "^5.1.1",
    "@fontsource-variable/reddit-mono": "^5.1.1",
    "@fontsource/iosevka-etoile": "^5.1.0",
    "@fontsource/monaspace-krypton": "^5.1.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/builder-vite": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^8.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "storybook": "^8.4.7",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-static-copy": "^2.2.0",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
