{
  "name": "dsssp-io",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "prebuild": "node -e \"process.stdout.write('VITE_BUILD_DATETIME=') + process.stdout.write(new Date().toISOString())\" > .env",
    "prettier": "prettier --check ./**/*.{ts,tsx,js,jsx}",
    "prettier:fix": "prettier  --write ./**/*.{ts,tsx,js,jsx}",
    "eslint": "eslint \"./**/*.{ts,tsx,js,jsx}\"",
    "eslint:fix": "eslint --fix \"./**/*.{ts,tsx,js,jsx}\"",
    "eslint:complexity": "eslintcc -sr -a -mr=F -mar=F -gt=B \"./**/*.{ts,tsx,js}\"",
    "eslint:ci": "eslint \"./**/*.{ts,tsx,js}\" -f node_modules/eslint-html-reporter/reporter.js -o eslint/report.html",
    "tsc": "tsc --noEmit -p tsconfig.json"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslintcc": "^0.8.3",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^5.4.2"
  }
}
