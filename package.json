{
  "name": "dsssp-io",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "generate-docs": "node scripts/generate-docs.js",
    "prettier": "prettier --check ./**/*.{ts,tsx,js,jsx}",
    "prettier:fix": "prettier  --write ./**/*.{ts,tsx,js,jsx}",
    "eslint": "eslint \"./**/*.{ts,tsx,js,jsx}\"",
    "eslint:fix": "eslint --fix \"./**/*.{ts,tsx,js,jsx}\""
  },
  "main": "dist/dsssp-io.umd.js",
  "module": "dist/dsssp-io.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write --cache",
      "eslint --fix --cache"
    ]
  },
  "resolutions": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "react": "^18.0.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
